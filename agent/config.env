# config.env
# SmartDNS Log Agent 配置文件

# 节点配置
NODE_ID=1
NODE_NAME=node-1

# 审计日志文件路径
LOG_FILE=/var/log/audit/audit.log

# 批处理配置
BATCH_SIZE=1000
FLUSH_INTERVAL_SEC=2

# ClickHouse 配置
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_DB=smartdns_logs
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=